{
  "name": "floor12/yii2-module-files",
  "description": "Yii2 module to upload and manage files to your models.",
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "keywords": [
    "floor12",
    "yii2 module",
    "yii",
    "yii2",
    "yii 2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "floor12",
      "email": "primelifecoder@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "ext-gd": "*",
    "ext-zip": "*",
    "ext-exif": "*",
    "yiisoft/yii2": "~2.0.13",
    "yiisoft/yii2-jui": "*",
    "yii2mod/yii2-enum": "^1.7",
    "floor12/yii2-notification": "*",
    "bower-asset/cropper": "*",
    "bower-asset/lightbox2": "*",
    "bower-asset/simple-ajax-uploader": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "floor12\\files\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "floor12\\files\\tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
