{
  "name": "floor12/yii2-module-files",
  "description": "Yii2 module to upload and manage files to your models.",
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "keywords": [
    "floor12",
    "yii2 module",
    "yii",
    "yii2",
    "yii 2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "floor12",
      "email": "primelifecoder@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": "~2.0.13",
    "bower-asset/cropper": "*",
    "floor12/yii2-notification": "dev-master",
    "rmrevin/yii2-fontawesome": "~2.15",
    "bower-asset/lightbox2": "*",
    "bower-asset/simple-ajax-uploader": "~2.6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "floor12\\files\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "floor12\\files\\tests\\": "tests/"
    }
  }
}
